class student:
    English=None
    Tamil=None
    Maths=None
    Science=None
    SocialScience=None
    ishalf=None
    def _init_(self,English,Tamil,Maths,Science,SocialScience):
        self.English=English
        self.Maths=Maths
        self.Tamil=Tamil
        self.Science=Science
        self.SocialScience=SocialScience
    def avg(self):
        return (self.English+self.Tamil+self.Maths+self.SocialScience+self.Science)/5
no=input('Enter the number of students')
sl=[]
for i in range(no):
    print(f"Enter {0}st students Quarterly marks",i)
    e=int(input("English"))
    t=int(input("Tamil"))
    m=int(input("Maths"))
    s=int(input("Science"))
    ss=int(input("SocialScience"))
    s=student(e,t,m,s,ss,0)
    sl.append(s)
    print(f"Enter {0}st students half marks", i)
    e = int(input("English"))
    t = int(input("Tamil"))
    m = int(input("Maths"))
    s = int(input("Science"))
    ss = int(input("SocialScience"))
    s = student(e, t, m, s, ss,1)
    sl.append(s)
for i in sl:
    print("studen")
