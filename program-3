import java.util.*;
public class Hello {

    public static void main(String[] args) {
	Scanner sc=new Scanner(System.in);
    System.out.print("Enter x and y\n");
    int x=sc.nextInt();
    int y=sc.nextInt();
    System.out.printf("Before Swapping\n");
    System.out.print("x="+x+"\n");
    System.out.print("y="+y+"\n");
    x=x+y;
    y=x-y;
    x=x-y;
    System.out.print("After Swapping without a third variable\n");
    System.out.print("x="+x+"\n");
    System.out.print("y ="+y);


	}
}
